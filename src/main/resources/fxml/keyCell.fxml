<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<HBox fx:id="cell" alignment="CENTER" styleClass="no-bg" stylesheets="@../style/gui.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <StackPane>
         <children>
            <ImageView fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../style/key.png" />
               </image>
            </ImageView>
            <Label fx:id="keyChar" alignment="TOP_LEFT" contentDisplay="CENTER" text="A" textFill="WHITE" translateX="10.0" translateY="10.0" StackPane.alignment="TOP_LEFT">
               <font>
                  <Font size="16.0" />
               </font></Label>
         </children>
      </StackPane>
      <Label fx:id="time" />
   </children>
</HBox>
